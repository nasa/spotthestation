    "jest": "^29.2.1",
    "jest-circus": "29",
    "jest-environment-node": "29",
    "jest-expo": "^47.0.0",
    "metro-config": "0.72.3",
    "metro-react-native-babel-preset": "0.73.10",
    "metro-source-map": "0.72.3",
    "mocha": "6",
    "mockdate": "^3.0.5",
    "patch-package": "6.4.7",
    "postinstall-prepare": "1.0.1",
    "prettier": "2.6.2",
    "query-string": "^7.0.1",
    "react-devtools-core": "4.24.7",
    "react-dom": "18.1.0",
    "react-native-dotenv": "^3.4.6",
    "reactotron-core-client": "^2.8.10",
    "ts-jest": "29",
